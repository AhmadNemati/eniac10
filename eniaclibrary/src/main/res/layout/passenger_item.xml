<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dialog"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    app:cardBackgroundColor="@color/colorPrimary_"
    app:cardCornerRadius="6dp"
    app:cardElevation="3dp"
    android:layout_marginLeft="@dimen/_5sdp"
    android:layout_marginRight="@dimen/_5sdp"
    android:layout_marginTop="@dimen/_5sdp"
    android:layout_marginBottom="@dimen/_5sdp"
    android:focusableInTouchMode="true"
    android:descendantFocusability="beforeDescendants">

  <!--  <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">-->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|top"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/tvCount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:gravity="center"
                android:textSize="14dp"
                android:textAppearance="@style/IranSansBold"
                android:text="sadcasd"
                android:textColor="@color/currentColor"/>
            <LinearLayout
                android:id="@+id/llPass"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center|right"
                android:layout_marginTop="5dp"
                >

                <android.support.design.widget.TextInputLayout
                    android:layout_width="0dp"
                    android:layout_weight="100"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_gravity="center|right"
                    android:theme="@style/EditTextHint"
                    android:layout_marginRight="5dp"


                    >
                    <library.android.eniac.utility.ClearableEditText
                        android:id="@+id/etLastName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100"
                        android:textSize="12dp"
                        android:inputType="text"
                        android:gravity="right|center"
                        android:backgroundTint="@color/textColor2"
                        android:textColor="@color/textColor2"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:hint="نام خانوادگی"
                        android:imeOptions="actionNext"




                        />
                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:layout_width="0dp"
                    android:layout_weight="100"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_gravity="center|right"
                    android:theme="@style/EditTextHint"
                    android:layout_marginRight="5dp"


                    >
                    <library.android.eniac.utility.ClearableEditText
                        android:id="@+id/etFirstName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100"
                        android:textSize="12dp"
                        android:inputType="text"
                        android:gravity="right|center"
                        android:backgroundTint="@color/textColor2"
                        android:textColor="@color/textColor2"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:hint="نام"
                        android:imeOptions="actionNext"



                        />
                </android.support.design.widget.TextInputLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_16sdp"
                    android:textAppearance="@style/IconFonts"
                    android:text="@string/profile_icon"
                    android:textColor="@color/currentColor"
                    android:layout_marginRight="5dp"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center|right"
                android:orientation="horizontal">

                <android.support.design.widget.TextInputLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_gravity="center|right"
                    android:layout_marginRight="5dp"
                    android:layout_weight="100"
                    android:theme="@style/EditTextHint"


                    >

                    <library.android.eniac.utility.ClearableEditText
                        android:id="@+id/etLastNameEn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100"
                        android:backgroundTint="@color/textColor2"
                        android:gravity="right|center"
                        android:hint="نام خانوادگی(انگلیسی)"
                        android:inputType="text"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:textColor="@color/textColor2"
                        android:textSize="12dp"
                        android:imeOptions="actionNext"



                        />
                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_gravity="center|right"
                    android:layout_marginRight="5dp"
                    android:layout_weight="100"
                    android:theme="@style/EditTextHint"


                    >

                    <library.android.eniac.utility.ClearableEditText
                        android:id="@+id/etFirstNameEn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100"
                        android:backgroundTint="@color/textColor2"
                        android:gravity="right|center"
                        android:hint="نام(انگلیسی)"
                        android:inputType="text"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:textColor="@color/textColor2"
                        android:textSize="12dp"
                        android:imeOptions="actionNext"



                        />
                </android.support.design.widget.TextInputLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/profile_icon"
                    android:textAppearance="@style/IconFonts"
                    android:textColor="@color/currentColor"
                    android:textSize="@dimen/_16sdp"
                    android:layout_marginRight="5dp"
                    />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center|right"
                android:orientation="horizontal">

                <android.support.design.widget.TextInputLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_gravity="center|right"
                    android:layout_marginRight="5dp"
                    android:layout_weight="100"
                    android:theme="@style/EditTextHint"


                    >

                    <library.android.eniac.utility.ClearableEditText
                        android:id="@+id/etIdType"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100"
                        android:backgroundTint="@color/textColor2"
                        android:gravity="right|center"
                        android:hint="نوع مدرک شناسائی"
                        android:imeOptions="actionNext"
                        android:inputType="text"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:textColor="@color/textColor2"
                        android:textSize="12dp"


                        />
                </android.support.design.widget.TextInputLayout>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/profile_icon"
                    android:textAppearance="@style/IconFonts"
                    android:textColor="@color/currentColor"
                    android:textSize="@dimen/_16sdp"
                    android:layout_marginRight="5dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center|right"
                android:orientation="horizontal">

                <android.support.design.widget.TextInputLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_gravity="center|right"
                    android:layout_marginRight="5dp"
                    android:layout_weight="100"
                    android:theme="@style/EditTextHint"


                    >

                    <library.android.eniac.utility.ClearableEditText
                        android:id="@+id/etExpireDate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100"
                        android:backgroundTint="@color/textColor2"
                        android:gravity="right|center"
                        android:hint="تاریخ انقضاء پاسپورت"
                        android:imeOptions="actionNext"
                        android:inputType="text"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:textColor="@color/textColor2"
                        android:textSize="12dp"


                        />
                </android.support.design.widget.TextInputLayout>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/profile_icon"
                    android:textAppearance="@style/IconFonts"
                    android:textColor="@color/currentColor"
                    android:textSize="@dimen/_16sdp"
                    android:layout_marginRight="5dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center|right"
                android:layout_marginTop="5dp">

                <android.support.design.widget.TextInputLayout
                    android:layout_width="0dp"
                    android:layout_weight="100"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_gravity="center|right"
                    android:theme="@style/EditTextHint"
                    android:layout_marginRight="5dp"


                    >
                    <library.android.eniac.utility.ClearableEditText
                        android:id="@+id/etNationality"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100"
                        android:textSize="12dp"
                        android:inputType="text"
                        android:gravity="right|center"
                        android:backgroundTint="@color/textColor2"
                        android:textColor="@color/textColor2"
                        android:imeOptions="actionNext"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:hint="ملیت"



                        />
                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:id="@+id/tilGender"
                    android:layout_width="0dp"
                    android:layout_weight="100"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_gravity="center|right"
                    android:theme="@style/EditTextHint"
                    android:layout_marginRight="5dp"


                    >
                    <library.android.eniac.utility.ClearableEditText
                        android:id="@+id/etGender"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100"
                        android:textSize="12dp"
                        android:inputType="text"
                        android:gravity="right|center"
                        android:backgroundTint="@color/textColor2"
                        android:textColor="@color/textColor2"
                        android:imeOptions="actionNext"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:hint="جنسیت"



                        />
                </android.support.design.widget.TextInputLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_16sdp"
                    android:textAppearance="@style/IconFonts"
                    android:text="@string/calendar_icon"
                    android:textColor="@color/currentColor"
                    android:layout_marginRight="5dp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center|right"
                android:layout_marginTop="5dp">

                <android.support.design.widget.TextInputLayout
                    android:layout_width="0dp"
                    android:layout_weight="100"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_gravity="center|right"
                    android:theme="@style/EditTextHint"
                    android:layout_marginRight="5dp"


                    >
                    <library.android.eniac.utility.ClearableEditText
                        android:id="@+id/etBirthDay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100"
                        android:textSize="12dp"
                        android:inputType="text"
                        android:gravity="right|center"
                        android:backgroundTint="@color/textColor2"
                        android:textColor="@color/textColor2"
                        android:imeOptions="actionNext"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:hint="تاریخ تولد"



                        />
                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:layout_width="0dp"
                    android:layout_weight="100"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_gravity="center|right"
                    android:theme="@style/EditTextHint"
                    android:layout_marginRight="5dp"


                    >
                    <library.android.eniac.utility.ClearableEditText
                        android:id="@+id/etNationalCode"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="100"
                        android:textSize="12dp"
                        android:inputType="text"
                        android:gravity="right|center"
                        android:backgroundTint="@color/textColor2"
                        android:textColor="@color/textColor2"
                        android:imeOptions="actionNext"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:hint="کد ملی"



                        />
                </android.support.design.widget.TextInputLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_16sdp"
                    android:textAppearance="@style/IconFonts"
                    android:text="@string/calendar_icon"
                    android:textColor="@color/currentColor"
                    android:layout_marginRight="5dp"
                    />

            </LinearLayout>


        </LinearLayout>

<!--
    </android.support.v4.widget.NestedScrollView>
-->


</android.support.v7.widget.CardView>